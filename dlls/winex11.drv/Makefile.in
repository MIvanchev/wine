	init.c \
	keyboard.c \
	mouse.c \
	opengl.c \
	palette.c \
	pen.c \
	settings.c \
	systray.c \
	vulkan.c \
	window.c \
	wintab.c \
	x11drv_main.c \
	xdnd.c \
	xim.c \
	xinerama.c \
	xrandr.c \
	xrender.c \
	xvidmode.c

RC_SRCS = version.rc
